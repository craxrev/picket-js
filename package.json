{
  "name": "@picketapi/picket-js",
  "version": "0.0.37",
  "description": "Javascript client for the Picket API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "module": "dist/module.js",
  "global": "dist/global.js",
  "targets": {
    "main": {
      "context": "browser"
    },
    "module": {
      "context": "browser"
    },
    "global": {
      "context": "browser",
      "isLibrary": false,
      "outputFormat": "global",
      "includeNodeModules": true
    }
  },
  "scripts": {
    "build": "npx parcel build",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/picketapi/picket-js.git"
  },
  "author": "picket",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/picketapi/picket-js/issues"
  },
  "homepage": "https://github.com/picketapi/picket-js#readme",
  "dependencies": {
    "@coinbase/wallet-sdk": "^3.0.6",
    "@solana/wallet-adapter-base": "^0.9.5",
    "@solana/wallet-adapter-wallets": "^0.16.1",
    "@types/bs58": "^4.0.1",
    "@walletconnect/web3-provider": "^1.7.7",
    "bs58": "^4.0.1",
    "eth-provider": "^0.10.0",
    "ethers": "^5.6.0",
    "pkce-challenge": "^3.0.0",
    "typescript": "^4.6.2",
    "web3modal": "^1.9.7"
  },
  "devDependencies": {
    "@parcel/packager-ts": "^2.4.1",
    "@parcel/transformer-typescript-types": "^2.4.1",
    "crypto-browserify": "^3.12.0",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "https-browserify": "^1.0.0",
    "os-browserify": "^0.3.0",
    "parcel": "^2.4.1",
    "prettier": "^2.5.1",
    "querystring-es3": "^0.2.1",
    "stream-http": "^3.2.0",
    "url": "^0.11.0"
  },
  "keywords": [
    "web3",
    "authentication"
  ]
}
